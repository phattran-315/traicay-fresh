{
  "name": "traicayfresh",
  "version": "0.1.0",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "dev:next": "next dev",
    "native:build": "next build",
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts nodemon",
    "build:native:next": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NEXT_BUILD=true next build",
    "start": "next start",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NEXT_BUILD=true node dist/server/server.js",
    "build": "cross-env NODE_ENV=production npm run build:native:next &&  npm run build:payload && npm run build:server && npm run copyfiles && npm run build:next",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,js}\" dist/",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NODE_ENV=production node dist/server/server.js",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload generate:types",
    "lint": "next lint",
    "test:open": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts  dotenv dotenv -e .env.test   -- concurrently \"dotenv -e .env.test npm run dev\" \"cypress open\" ",
    "test:run": "cross-env  PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts dotenv dotenv -e .env.test   -- concurrently \"dotenv -e .env.test npm run dev\"  "
  },
